<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:context="http://www.springframework.org/schema/context"   
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
	http://www.springframework.org/schema/context
	http://www.springframework.org/schema/context/spring-context-2.5.xsd">
    
    <context:annotation-config/>

    <bean id="imagesBean" class="com.intel.director.images.Images"/>
    <bean id="imageServiceBean"  class="com.intel.director.service.impl.ImageServiceImpl"/>
    <bean id="imagePersistenceManagerBean"  class="com.intel.mtwilson.director.dbservice.DbServiceImpl"/>
    <bean id="imageStoreBean"  class="com.intel.director.service.impl.ImageStoreManagerImpl"/>
    <bean id="lookupServiceBean"  class="com.intel.director.service.impl.LookupServiceImpl"/>
</beans>
